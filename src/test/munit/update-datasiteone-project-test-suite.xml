    <munit:config name="munit" doc:name="MUnit configuration"/>
    <spring:beans>
        <spring:import resource="classpath:globals.xml"/>
        <spring:import resource="classpath:upserts/update-datasiteone-project.xml"/>
    </spring:beans>
    <munit:test name="update-datasiteone-project-test-suite-updateDatasiteOneProjectFlowTest" description="Test">
        <mock:when messageProcessor=".*:.*" doc:name="Mock Salesforce">
            <mock:with-attributes>
                <mock:with-attribute name="doc:name" whereValue="Salesforce"/>
            </mock:with-attributes>
            <mock:then-return payload="[{&quot;status&quot;:&quot;S&quot;}]" mimeType="application/json"/>
        </mock:when>
        <set-payload value="#[&quot;What is supposed to be here?&quot;]" doc:name="Set Payload"/>
        <flow-ref name="updateDatasiteOneProjectFlow" doc:name="updateDatasiteOneProjectFlow"/>
        <dw:transform-message doc:name="Transform Message">
            <dw:set-payload><![CDATA[%dw 1.0
%output application/java
---
payload]]></dw:set-payload>
        </dw:transform-message>
        <munit:assert-true message="failed in update aria creation field" condition="#[payload.size() &gt; 0]" doc:name="Assert True"/>
    </munit:test>
</mule>
